{
    "printWidth": 120,
    "semi": true,
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "all",
    "singleQuote": false,
    "endOfLine": "lf",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "quoteProps": "as-needed",
    "experimentalTernaries": true,
    "plugins": [
        "prettier-plugin-organize-imports"
    ]
}
